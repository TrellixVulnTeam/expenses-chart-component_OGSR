*COLORS*/\r\n\r\n$dark-brown: hsl(25, 47%, 15%);\r\n$medium-brown: hsl(28, 10%, 53%);\r\n$cream: hsl(27, 66%, 92%);\r\n$very-pale-orange: hsl(33, 100%, 98%);\r\n$soft-red: hsl(10, 79%, 65%);\r\n$cyan: hsl(186, 34%, 60%);\r\n\r\n/*FONT-WEIGHTS*/\r\n$medium: 400;\r\n$bold: 700;\r\n\r\n/*FONT-SIZE*/\r\n$font-size: 18px;\r\n$bar-chart-font-size: 12px;\r\n\r\n/*RADIUS*/\r\n$border-radius-balance: 13px;\r\n$border-radius-bar: 5px;\r\n\r\n/* BREAKPOINTS*/\r\n$breakpoints: (\r\n  'xs': 0,\r\n  'sm': 480px,\r\n  'md': 720px,\r\n  'lg': 960px,\r\n  'xl': 1200px\r\n);\r\n\r\n/*MIXINS*/\r\n@mixin xs {\r\n  @media (min-width: map-get($breakpoints, 'xs')) {\r\n    @content;\r\n  }\r\n}\r\n@mixin sm {\r\n  @media (min-width: map-get($breakpoints, 'sm')) {\r\n    @content;\r\n  }\r\n}\r\n@mixin md {\r\n  @media (min-width: map-get($breakpoints, 'md')) {\r\n    @content;\r\n  }\r\n}\r\n@mixin lg {\r\n  @media (min-width: map-get($breakpoints, 'lg')) {\r\n    @content;\r\n  }\r\n}\r\n@mixin xl {\r\n  @media (min-width: map-get($breakpoints, 'xl')) {\r\n    @content;\r\n  }\r\n}\r\n\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  height: 100vh;\r\n  background-color: $cream;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  font-family: \"DM Sans\", sans-serif;\r\n}\r\n\r\nh1 {\r\n  text-align: left;\r\n  font-weight: $bold;\r\n  @include sm {\r\n    font-size: 2.5rem;\r\n  }\r\n}\r\nh2 {\r\n  @include sm {\r\n    font-size: 2rem;\r\n  }\r\n}\r\n\r\nh3 {\r\n  font-size: 1.5rem;\r\n  margin-bottom: .5rem;\r\n}\r\n\r\np {\r\n  font-size: $font-size;\r\n  font-weight: $medium;\r\n}\r\n\r\n.container {\r\n  width: 90%;\r\n  height: 80%;\r\n  background: transparent;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  @include sm {\r\n      width: 30rem;\r\n      height: 40rem;\r\n  }\r\n}\r\n\r\n.balance-wrapper {\r\n  width: 100%;\r\n  height: 18%;\r\n  display: inline-flex;\r\n  justify-content: space-between;\r\n  background-color: $soft-red;\r\n  color: $very-pale-orange;\r\n  border: 1px solid transparent;\r\n  border-radius: $border-radius-balance;\r\n  padding: 1.5em;\r\n  margin-bottom: 1em;\r\n  .balance-items {\r\n    line-height: 1.5;\r\n  }\r\n\r\n  img {\r\n    height: 60px;\r\n    width: 60px;\r\n  }\r\n}\r\n\r\n.chart-total-wrapper {\r\n  height: 72%;\r\n  width: 100%;\r\n  background-color: $very-pale-orange;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  border: 1px solid transparent;\r\n  border-radius: $border-radius-balance;\r\n  padding: 1.5em;\r\n  color: $dark-brown;\r\n}\r\n\r\n.chart-wrapper {\r\n  width: 100%;\r\n  height: 70%;\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n  align-items: end;\r\n\r\n  .bar-chart {\r\n    border: 1px solid transparent;\r\n    border-radius: $border-radius-bar;\r\n    background-color: $soft-red;\r\n    margin-bottom: 3.5rem;\r\n  }\r\n\r\n  .bar-chart:hover {\r\n    opacity: .7;\r\n    cursor: pointer;\r\n  }\r\n\r\n  .active {\r\n    background-color: $cyan;\r\n  }\r\n\r\n  span {\r\n    position: relative;\r\n    font-size: $bar-chart-font-size;\r\n    color: $medium-brown;\r\n    height: 25px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  .span-amount {\r\n    background: black;\r\n    color: white;\r\n    padding: .3rem;\r\n    border: 1px solid black;\r\n    border-radius: $border-radius-bar;\r\n    width: max-content;\r\n    transform: translateY(-60px);\r\n    transition: transform .3s ease-in-out;\r\n  }\r\n\r\n}\r\n\r\n.total-wrapper {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  line-height: 1.5;\r\n  padding-top: 1.5em;\r\n  border-top: 2px solid $cream;\r\n\r\n  .last-month {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: flex-end;\r\n    align-items: flex-end;\r\n  }\r\n\r\n  .last-month :nth-child(1) {\r\n    font-weight: $bold;\r\n  }\r\n\r\n  .last-month :nth-child(2),\r\n  .total-month :nth-child(1) {\r\n    font-size: 16px;\r\n    color: $medium-brown;\r\n  }\r\n\r\n}\r\n\r\n.footer {\r\n  width: 100%;\r\n  height: 30px;\r\n  bottom: 5px;\r\n  margin-top: 0;\r\n  position: fixed;\r\n\r\n  .attribution {\r\n    font-size: 11px;\r\n    text-align: center;\r\n  }\r\n\r\n  .attribution a {\r\n    color: hsl(228, 45%, 44%);\r\n  }\r\n}\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"74fa5c26136764d7d18a7cc2090b67ae","cacheData":{"env":{}}}